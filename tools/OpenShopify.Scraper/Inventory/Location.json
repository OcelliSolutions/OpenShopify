  "components": [
    {
      "name": "Location",
      "title": "Location",
      "type": "object",
      "properties": [
        {
          "description": "Whether the location is active. If <code>true</code>, then the location can be used to sell products,\nstock inventory, and fulfill orders. Merchants can deactivate locations from the Shopify admin.\nDeactivated locations don't contribute to the shop's\n<a href=\"https://help.shopify.com/manual/locations/setting-up-your-locations\">location limit</a>.\n",
          "type": "string",
          "example": true,
          "name": "active"
        },
        {
          "description": "The location's street address.",
          "type": "string",
          "example": "126 York Street",
          "name": "address1"
        },
        {
          "description": "The optional second line of the location's street address.",
          "type": "string",
          "example": "Unit 42",
          "name": "address2"
        },
        {
          "description": "The city the location is in.",
          "type": "string",
          "example": "Ottawa",
          "name": "city"
        },
        {
          "description": "The country the location is in.",
          "type": "string",
          "example": "Canada",
          "name": "country"
        },
        {
          "description": "The two-letter code (<a href=\"https://en.wikipedia.org/wiki/ISO_3166-1_alpha-2\" target=\"_blank\">ISO 3166-1 alpha-2</a> format) corresponding to country the location is in.",
          "type": "string",
          "example": "CA",
          "name": "country_code"
        },
        {
          "description": "The date and time (<a href=\"https://en.wikipedia.org/wiki/ISO_8601\" target=\"_blank\">ISO 8601 format</a>) when the location was created.",
          "type": "string",
          "example": "2008-12-31T17:00:00-07:00",
          "name": "created_at"
        },
        {
          "description": "The ID of the location.",
          "type": "string",
          "example": 121,
          "name": "id"
        },
        {
          "description": "          Whether this is a fulfillment service location. If <code>true</code>, then the location is a\n          fulfillment service location. If <code>false</code>, then the location was created by the\n          merchant and isn't tied to a fulfillment service.\n",
          "type": "string",
          "example": true,
          "name": "legacy"
        },
        {
          "description": "The name of the location.",
          "type": "string",
          "example": "Ottawa Store",
          "name": "name"
        },
        {
          "description": "The phone number of the location. This value can contain special characters, such as <code>-</code> or <code>+</code>.",
          "type": "string",
          "example": "18883290139",
          "name": "phone"
        },
